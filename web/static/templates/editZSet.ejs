<div id="itemActionsBar">
  <button class="btn btn-danger" onclick="deleteKey('<%= key %>')">Delete Key</button>
</div>
<div id="itemData">
  <table class="table">
    <thead>
      <th>#</th>
      <th>Score</th>
      <th>Value</th>
    </thead>
    <tbody>
      <% items.forEach(function(item) { %>
        <tr>
          <td><%= item.number %></td>
          <td><%= item.score %></td>
          <td><%= item.value %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
</div>